{
  "name": "gw2-embeds",
  "description": "Embedding script for discretize-gw2-ui",
  "version": "0.4.1",
  "main": "dist/gw2-embeds.js",
  "types": "dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "start": "react-scripts start",
    "prepare": "yarn build",
    "test": "yarn format && yarn lint && tsc --noEmit",
    "format": "prettier -w src",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "types": "yarn tsc --emitDeclarationOnly --declaration --declarationDir dist/@types --skiplibCheck",
    "build:dev": "webpack --config webpack/webpack.config.js --mode=development",
    "build": "webpack --config webpack/webpack.config.js --mode=production --node-env=production && yarn types"
  },
  "dependencies": {
    "@discretize/gw2-ui-new": "../../../@discretize-ui/gw2-ui",
    "@discretize/typeface-menomonia": "^0.1.3",
    "clsx": "^1.2.1",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^18.15.3",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.2",
    "postcss-preset-env": "^8.4.2",
    "prettier": "^2.7.0",
    "react-scripts": "^5.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.3.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "<5.0.0",
    "typescript-plugin-css-modules": "^4.2.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
