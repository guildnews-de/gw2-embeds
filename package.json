{
  "name": "gw2-embeds",
  "description": "Embedding script for discretize-gw2-ui",
  "version": "0.2.0",
  "main": "src/index.ts",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack --mode=production --node-env=production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier -w src",
    "storybook": "start-storybook --docs -p 6006 -s public",
    "build-storybook": "yarn build:dev && build-storybook --docs -s public",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@discretize/gw2-ui-new": "^0.0.19",
    "@discretize/typeface-menomonia": "^0.1.3",
    "clsx": "^1.2.1",
    "object-hash": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.15",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^28.1.1",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^17.0.43",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^5.28.0",
    "@webpack-cli/generators": "^2.5.0",
    "@wordpress/eslint-plugin": "^12.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.10",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-js": "^4.0.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.0",
    "react-scripts": "^5.0.1",
    "storybook-dark-mode": "^2.0.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
