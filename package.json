{
  "name": "gw2-embeds",
  "description": "Embedding script for discretize-gw2-ui",
  "version": "0.4.1",
  "main": "dist/gw2-embeds.js",
  "types": "dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "start": "react-scripts start",
    "prepare": "yarn build",
    "test": "yarn format && yarn lint && tsc --noEmit",
    "format": "prettier -l -w src",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "types": "yarn tsc --emitDeclarationOnly --declaration --declarationDir dist/@types --skiplibCheck",
    "build:dev": "webpack --config webpack/webpack.config.js --mode=development",
    "build": "webpack --config webpack/webpack.config.js --mode=production --node-env=production && yarn types"
  },
  "dependencies": {
    "@discretize/gw2-ui-new": "^0.1.4",
    "@discretize/typeface-menomonia": "^0.1.3",
    "clsx": "^2.1.0",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^20.11.20",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^9.4.0",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
